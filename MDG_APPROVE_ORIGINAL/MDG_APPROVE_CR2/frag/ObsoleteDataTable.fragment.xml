<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:sap.ui.core="sap.ui.core"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.layout.form="sap.ui.layout.form">
	<Table class="headerBold">
		<columns>
			<Column mergeDuplicates="true">
				<header>
					<Label text="{i18n>ConDescr}" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
				<header>
					<Label text="{i18n>DelEnDescr}" />
				</header>
			</Column>
			<!-- Extends the Table columns of a change table -->
			<core:ExtensionPoint name="extChangeDelTableColumns" />
		</columns>
	</Table>
</core:FragmentDefinition>