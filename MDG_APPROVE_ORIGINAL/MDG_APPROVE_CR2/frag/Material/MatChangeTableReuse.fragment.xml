<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:sap.ui.core="sap.ui.core"
	xmlns:sap.ui.layout="sap.ui.layout" xmlns:sap.ui.layout.form="sap.ui.layout.form">
	<Table >
		<columns>
			<Column mergeDuplicates="true" mergeFunctionName="getTitle">
				<header>
					<Text text="{i18n>ConDescr}" />
				</header>
			</Column>
			<Column  minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="{i18n>NEW_VALUE}" />
				</header>
			</Column>
			<Column  minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="{i18n>OldValue}" />
				</header>
			</Column>
			<!-- Extends the Table columns of a change table -->
			<core:ExtensionPoint name="extChangeTableColumns" />
		</columns>
	</Table>
</core:FragmentDefinition>